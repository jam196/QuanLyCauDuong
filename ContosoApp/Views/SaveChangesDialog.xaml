<ContentDialog
    x:Class="QuanLyCauDuong.Views.SaveChangesDialog"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:local="using:QuanLyCauDuong.Views"
    Title="Lưu thay đổi?"
    Content="Các thông tin bạn vừa nhập sẽ không được lưu lại?"
    PrimaryButtonText="Lưu"
    SecondaryButtonText="Không lưu"
    CloseButtonText="Hủy"
    PrimaryButtonClick="ContentDialog_PrimaryButtonClick"
    SecondaryButtonClick="ContentDialog_SecondaryButtonClick"
    CloseButtonClick="ContentDialog_CloseButtonClick"
    mc:Ignorable="d">
    <ContentDialog.Resources>
        <Style TargetType="local:SaveChangesDialog" BasedOn="{StaticResource DefaultContentDialogStyle}"/>
    </ContentDialog.Resources>
</ContentDialog>
